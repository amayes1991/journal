<%- include("partials/userheader"); -%>

<h1 class="userhome">
  <i class="fa-solid fa-book"></i>
  <%=firstName%>'s Journal</h1>

<section class="blog-text">




<% foundUser.forEach(function(user){  %>






<div class="notes container col-16" >




<h2 class="userTitle" id="user-title-_id"> <%= user.title %> </h2>
<p class="user-content" id="user-content-_id">
  <%= user.content %>

</p>
<a name="<%= user.title %>" class="delete-button"href="/userhome/notes/<%=user._id %>">Edit</a>

<form id="delete-link" class="delete-button" action=/api/<%=user._id %>/<%=logId%> method="post">
  <input type="hidden" name="_method" value="delete">
  <button type="submit" name="button" class="delete-button" value="<%= user.title %>">Delete</button>

</form>




  </div>

<hr>





<% })  %>




</section>
     <!-- </div> -->
<%- include("partials/userfooter"); -%>
